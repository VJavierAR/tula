<odoo>
    <data>

        <record model="ir.ui.view" id="res_partner_form_view_update_2">
            <field name="name">res.partner.form.update_2</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">

                <xpath expr="//page[@name='sales_purchases']" position="after">
                    <page string="Tramites" >

                        <field name="historal_fac_tramitadas" readonly="1">
                            <tree>
                                <field name="factura" />
                                <field name="tramite_file" />
                            </tree>
                            <form>
                                <group>
                                    <group>
                                        <field name="factura" />
                                    </group>
                                    <group>
                                        <field name="tramite_file" widget="many2many_binary" />
                                    </group>
                                </group>
                            </form>
                        </field>

                    </page>
                </xpath>

            </field>
        </record>
    </data>
</odoo>